{
  "name": "stocker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build:native": "node-gyp rebuild",
    "start": "STOCKER_CONF=/var/www/stocker/conf node index.js",
    "dev": "STOCKER_CONF=/var/www/stocker/conf nodemon index.js",
    "migrate": "mkdir -p db&&sequelize db:migrate",
    "rollback": "sequelize db:migrate:undo",
    "test": "STOCKER_CONF=./test/config jest",
    "webpack": "webpack --mode production",
    "webpack-dev": "webpack",
    "dev-server": "webpack --env htdocs_root=/ --env api_dummy=yes && webpack serve --host 0.0.0.0 --env htdocs_root=/ --env api_dummy=yes",
    "get-update": "npm-check-updates -u && npm update && npm update --save-dev"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "bunyan": "^1.8.15",
    "express": "^4.17.1",
    "html-render": "git+https://github.com/suzutsuki0220/htmlRender.git",
    "js-utils": "git+https://github.com/suzutsuki0220/jsUtils.git",
    "node-addon-api": "^3.0.2",
    "sequelize": "^6.5.0",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "author": "suzutsuki0220",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^14.14.10",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-check-updates": "^10.3.1",
    "sass-loader": "^10.1.0",
    "sequelize-cli": "^6.2.0",
    "string-replace-loader": "^3.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
